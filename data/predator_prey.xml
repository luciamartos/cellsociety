<?xml version="1.0"?>
<game>
    <main>
        <title>Predator Prey</title>
        <rule>PredatorPrey</rule>
        <author>Will Long</author>
    </main>

    <parameter>
        <name>fish_breed</name>
        <value>2</value>
    </parameter>
    <parameter>
        <name>shark_breed</name>
        <value>4</value>
    </parameter>
    <parameter>
        <name>shark_starve</name>
        <value>6</value>
    </parameter>

    <celltype>
        <id>0</id>
        <name>Water</name>
    </celltype>
    <celltype>
        <id>1</id>
        <name>Algae</name>
    </celltype>
    <celltype>
        <id>2</id>
        <name>Fish</name>
    </celltype>
    <celltype>
        <id>3</id>
        <name>Shark</name>
    </celltype>

    <grid>
        <width>10</width>
        <height>10</height>
        <defaultid>0</defaultid>
        <cell>
            <id>1</id>
            <row>0</row>
            <col>0</col>
        </cell>
        <cell>
            <id>1</id>
            <row>1</row>
            <col>2</col>
        </cell>
        <cell>
            <id>1</id>
            <row>3</row>
            <col>4</col>
        </cell>
        <cell>
            <id>1</id>
            <row>4</row>
            <col>5</col>
        </cell>
    </grid>

</game>
