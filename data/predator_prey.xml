<?xml version="1.0"?>
<game>
    <main>
        <title>Wator World</title>
        <rule>WatorWorld</rule>
        <author>Will Long</author>
    </main>

    <parameter>
        <name>fish_breed</name>
        <min>0</min>
        <max>10</max>
        <current>2</current>
    </parameter>
    <parameter>
        <name>shark_breed</name>
        <min>0</min>
        <max>10</max>
        <current>4</current>
    </parameter>
    <parameter>
        <name>shark_starve</name>
        <min>0</min>
        <max>10</max>
        <current>6</current>
    </parameter>
    <parameter>
        <name>lifetime</name>
        <min>0</min>
        <max>10</max>
        <current>2</current>
    </parameter>

    <celltype>
        <id>0</id>
        <name>KELP</name>
    </celltype>
    <celltype>
        <id>1</id>
        <name>FISH</name>
    </celltype>
    <celltype>
        <id>2</id>
        <name>SHARK</name>
    </celltype>

    <grid>
        <width>128</width>
        <height>64</height>
        <defaultid>0</defaultid>
        <tiling>hexagon</tiling>
        <toroidal>true</toroidal>
        <fillmethod>percentage</fillmethod>
    </grid>

    <location>
        <id>0</id>
        <percent>50</percent>
    </location>
    <location>
        <id>1</id>
        <percent>25</percent>
    </location>
    <location>
        <id>2</id>
        <percent>25</percent>
    </location>


</game>
